<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.changhak.bookmanager.repository.AdminRepository">

    <select id="findByLoginId" resultType="com.changhak.bookmanager.domain.Admin">
        SELECT id, login_id, password, name
        FROM admin
        WHERE login_id = #{loginId}
    </select>

</mapper>